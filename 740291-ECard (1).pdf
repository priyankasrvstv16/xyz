        function placeholders() {
            $("form input:text").each(function () {
                $(this).val($(this).attr("placeholder"));
            });
            $(":text").focus(function () {
                if ($(this).val() == $(this).attr("placeholder")) {
                    $(this).val("");
                }
            });

            $(":text").blur(function () {
                if ($(this).val() == "") {
                    $(this).val($(this).attr("placeholder"));
                }
            });

            $("form input:password").each(function () {
                $(this).val($(this).attr("placeholder"));
            });
