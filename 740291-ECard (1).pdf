        $(document).on('click', '.slide-box-open', function () {
            $('.slideBox').hide().css({ 'width': 0 });
            $('body').css({ 'overflow': 'hidden' });
            var thisLink = $(this).attr('rel');
            var thisWidth = $(this).attr('data-width');
            $(thisLink).show().animate({ width: '100%' }, 200);

            if (!thisWidth) {
                $(thisLink + ' .slideContent').animate({ width: 600 }, 200);
            }
            else {
                $(thisLink + ' .slideContent').animate({ width: thisWidth }, 200);
            }
            $('.slideBox .slideBg').click(function () {
                $(this).parent().animate({ width: 0 }, 300).hide();
                $('body').css({ 'overflow': 'auto' });
            });
            $('.slideHeading a').click(function () {
                $(this).parent().parent().parent('.slideBox').animate({ width: 0 }, 100).hide();
                $('body').css({ 'overflow': 'auto' });
            });
        });

        function checkSSOCorproateorNot() {
            var username = $('#username').val();
            var empname = "";
            var corpname = "";
            if (username != null && username != "") {
                var mapping = JSON.parse(SSOCorpUrlMapping);
                empname = username.toLowerCase()
                corpname = username.toLowerCase();
                if (username != '' && username.indexOf('@') > -1) {
                    empname = username.split('@')[0].toLowerCase()
                    corpname = username.split('@')[1].toLowerCase();
                }
                if (mapping != null && empname.indexOf('hr') <= -1 && empname.indexOf('test') <= -1 && mapping[corpname] != null
                    && mapping[corpname] != ""
                    && mapping[corpname] !== undefined
                    && corpname.indexOf(mapping[corpname]) >= -1) {
                    $('#ErrorMsg').html('This functionality has been disabled for your corporate. Please <a href="' + mapping[corpname] + '"> Click here</a> to login through your corporate portal to access MediBuddy.');
                    $('#lblpassword').hide();
                    $('#password').hide();
                    $('#signinBtn').hide();
                    $('#ForgotCredentials').hide();
                }
                else {
                    $('#ErrorMsg').html('');
                    $('#lblpassword').show();
                    $('#password').show();
                    $('#signinBtn').show();
                    $('#ForgotCredentials').show();
                }
            }
        }
    </script>

